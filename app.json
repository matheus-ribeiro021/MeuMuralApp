{
  "expo": {
    "name": "MeuMuralApp",
    "slug": "MeuMuralApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/user-icon.png",
    "scheme": "meumuralapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/user-icon.png",
        "backgroundImage": "./assets/images/user-icon.png",
        "monochromeImage": "./assets/images/user-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.MeuMuralApp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/user-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/user-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63fe9075-8cb7-43af-91d1-c11f1a8572bf"
      }
    }
  }
}
